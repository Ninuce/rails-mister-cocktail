<div class="container">
<h1>Hi add a dose!</h1>
<%= simple_form_for [@cocktail, @dose] do |f| %>
  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.input :ingredient_id, collection: Ingredient.all %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
</div>
